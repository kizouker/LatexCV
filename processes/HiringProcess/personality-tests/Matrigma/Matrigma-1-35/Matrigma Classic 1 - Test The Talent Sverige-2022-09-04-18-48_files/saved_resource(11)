{var scroll_to_el=$(hash),modifier=0;if(scroll_to_el.length)
{the_win.on('scroll.avia_first_scroll',function()
{setTimeout(function()
{if(sticky_sub.length&&scroll_to_el.offset().top>sticky_sub.offset().top)
{modifier=sticky_sub.outerHeight()-3;}
